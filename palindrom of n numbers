#include<stdio.h>

int main()

{
    
	int n,rem,temp,rev=0;
  
  printf("Enter number=");
  
  scanf("%d",&n);
  
  temp=n;
  
  while(n>0)
    
	{
        
		rem=n%10;
        
		rev=rev*10+rem;
        
		n=n/10;
    
	}
    
	if(temp==rev)
    	
	{
        
		printf("Number is Palindrom=%d",rev);
    
	}
    
	else
    
	{
        
		printf("Number is not Palindrom");
    
	}
   
 	return 0;

}
